# Environment Variables for RAGRace
# Copy this file to .env and fill in your actual API keys

# RAG Provider API Keys
# LlamaIndex (uses OpenAI for embeddings/LLM)
OPENAI_API_KEY=your_openai_key_here

# LlamaIndex Cloud (LlamaParse for PDF parsing)
LLAMAINDEX_API_KEY=your_llamaindex_cloud_key_here

# LandingAI Agentic Document Extraction
VISION_AGENT_API_KEY=your_landingai_ade_api_key_here

# Reducto Document Processing
REDUCTO_API_KEY=your_reducto_api_key_here

# Other providers (if added)
PINECONE_API_KEY=your_pinecone_key_here

# LLM API Keys (for scoring)
ANTHROPIC_API_KEY=your_anthropic_key_here

# Optional: Add more provider API keys as needed
# NEW_PROVIDER_API_KEY=your_key_here
